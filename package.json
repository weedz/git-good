{
  "name": "git-good",
  "version": "1.0.0",
  "description": "Git good",
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_OPTIONS=\"--unhandled-rejections=strict\" electron .",
    "start:dev": "concurrently \"npm:build:watch\" \"npm:start:forge\"",
    "start:forge": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "build": "rm -r dist; rollup -c && cp -R static/* dist/",
    "build:watch": "mkdir dist; cp -R static/* dist/ && rollup -c -w",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": "Linus Bj√∂rklund <weedzcokie@gmail.com>",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.53",
    "@electron-forge/maker-deb": "^6.0.0-beta.53",
    "@electron-forge/maker-rpm": "^6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.53",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.53",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/nodegit": "^0.26.7",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@weedzcokie/router-tsx": "^0.1.6",
    "concurrently": "^5.3.0",
    "electron": "^10.1.2",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.22.0",
    "preact": "^10.4.8",
    "rollup": "^2.26.8",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.1",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "nodegit": "^0.27.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weedz/git-good.git"
  },
  "bugs": {
    "url": "https://github.com/weedz/git-good/issues"
  },
  "homepage": "https://github.com/weedz/git-good#readme"
}
