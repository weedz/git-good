{
  "name": "git-good",
  "version": "0.1.4",
  "description": "Git good",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "lint": "eslint . && tsc --noEmit",
    "dev": "tsc --noEmit --watch",
    "watch": "NODE_ENV=development npm run build",
    "build": "rm -rf dist;mkdir dist;cp static/* dist/; node scripts/build.mjs",
    "dist": "electron-builder",
    "electron-rebuild": "electron-rebuild"
  },
  "keywords": [],
  "author": {
    "name": "Linus BjÃ¶rklund",
    "email": "weedzcokie@gmail.com"
  },
  "files": [
    "dist"
  ],
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@datadog/build-plugin": "^1.0.3",
    "@electron/asar": "^3.2.2",
    "@electron/rebuild": "^3.2.10",
    "@types/node": "^18.11.9",
    "@types/nodegit": "^0.28.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "electron": "^21.2.3",
    "electron-builder": "^23.6.0",
    "esbuild": "^0.15.13",
    "eslint": "^8.27.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.10",
    "preact": "^10.11.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@weedzcokie/store": "^1.1.3",
    "nodegit": "github:weedz/nodegit#master"
  },
  "resolutions": {
    "nan": "github:weedz/nan#main",
    "@electron/universal": "^1.3.3"
  },
  "build": {
    "appId": "git-good",
    "afterPack": "./scripts/after-pack.js",
    "directories": {
      "output": "out"
    },
    "linux": {
      "category": "Development",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weedz/git-good.git"
  },
  "bugs": {
    "url": "https://github.com/weedz/git-good/issues"
  },
  "homepage": "https://github.com/weedz/git-good#readme",
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  }
}
