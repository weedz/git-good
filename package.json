{
    "name": "git-good",
    "version": "0.1.4",
    "description": "Git good",
    "main": "dist/main.js",
    "type": "commonjs",
    "scripts": {
        "start": "electron --ozone-platform-hint=wayland .",
        "lint": "oxlint -D correctness -D perf",
        "tsc:watch": "tsc --noEmit --watch",
        "dev": "NODE_ENV=development npm run build",
        "build": "rm -rf dist;mkdir dist;cp static/* dist/; node scripts/build.mjs",
        "dist": "electron-builder",
        "electron-rebuild": "JOBS=max CC=clang CXX=clang++ LINK=clang++ electron-rebuild"
    },
    "keywords": [],
    "author": {
        "name": "Linus BjÃ¶rklund",
        "email": "weedzcokie@gmail.com"
    },
    "files": [
        "dist"
    ],
    "license": "GPL-3.0-or-later",
    "devDependencies": {
        "@electron/asar": "^3.2.10",
        "@electron/rebuild": "^3.6.0",
        "@types/node": "^20.14.12",
        "@types/nodegit": "^0.28.7",
        "@weedzcokie/store": "^2.0.2",
        "electron": "^31.2.1",
        "electron-builder": "^24.13.3",
        "esbuild": "^0.23.0",
        "oxlint": "^0.6.1",
        "preact": "^10.23.0",
        "typescript": "^5.5.4"
    },
    "resolutions": {
        "make-fetch-happen": "^13"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "webpack",
                "electron-builder-squirrel-windows"
            ]
        }
    },
    "build": {
        "appId": "git-good",
        "afterPack": "./scripts/after-pack.js",
        "directories": {
            "output": "out"
        },
        "linux": {
            "category": "Development",
            "target": [
                "AppImage",
                "deb"
            ]
        },
        "mac": {
            "category": "public.app-category.developer-tools"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/weedz/git-good.git"
    },
    "bugs": {
        "url": "https://github.com/weedz/git-good/issues"
    },
    "homepage": "https://github.com/weedz/git-good#readme",
    "engines": {
        "node": ">=16",
        "pnpm": ">=7"
    },
    "dependencies": {
        "nodegit": "github:weedz/nodegit#master"
    },
    "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
