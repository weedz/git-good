    "build:watch": "mkdir dist; cp -R static/* dist/ && rollup -c -w",
    "electron-rebuild": "electron-builder install-app-deps",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": {
    "name": "Linus Bj√∂rklund",
    "email": "weedzcokie@gmail.com"
  },
  "files": [
    "dist"
  ],
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/node": "^16.0.0",
    "@types/nodegit": "^0.27.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "electron": "^13.1.4",
    "electron-builder": "^22.11.7",
    "eslint": "^7.29.0",
    "eslint-config-preact": "^1.1.4",
    "preact": "^10.5.13",
    "rollup": "^2.56.3",
    "rollup-plugin-import-css": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@electron/remote": "^1.2.0",
    "@weedzcokie/store": "^1.0.0",
    "nodegit": "^0.28.0-alpha.9",
    "uuid": "^8.3.2"
  },
  "build": {
