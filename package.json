    "build:watch": "mkdir dist; cp -R static/* dist/ && rollup -c -w",
    "electron-rebuild": "electron-builder install-app-deps",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": {
    "name": "Linus Bj√∂rklund",
    "email": "weedzcokie@gmail.com"
  },
  "files": [
    "dist"
  ],
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.0.0",
    "@types/node": "^14.14.7",
    "@types/nodegit": "^0.27.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "electron": "^13.0.0-beta.9",
    "electron-builder": "^22.9.1",
    "eslint": "^7.12.0",
    "eslint-config-preact": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "preact": "^10.5.6",
    "rollup": "^2.33.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.3.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@electron/remote": "^1.1.0",
    "nodegit": "^0.28.0-alpha.2",
    "uuid": "^8.3.2"
  },
  "build": {
    "directories": {
      "output": "out"
    },
    "linux": {
      "category": "Development",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weedz/git-good.git"
