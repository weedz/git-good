{
  "name": "git-good",
  "version": "0.0.7",
  "description": "Git good",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "NODE_OPTIONS=\"--unhandled-rejections=strict\" electron .",
    "dist": "electron-builder",
    "publish": "electron-builder --publish always",
    "build": "rm -r dist; rollup -c && cp -R static/* dist/",
    "build:watch": "mkdir dist; cp -R static/* dist/ && rollup -c -w",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": {
    "name": "Linus Bj√∂rklund",
    "email": "weedzcokie@gmail.com"
  },
  "files": [
    "dist"
  ],
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@types/node": "^14.14.7",
    "@types/nodegit": "^0.26.11",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "eslint": "^7.12.0",
    "eslint-config-preact": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "preact": "^10.5.6",
    "rollup": "^2.33.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "nodegit": "^0.27.0"
  },
  "build": {
    "directories": {
      "output": "out"
    },
    "linux": {
      "category": "Development",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weedz/git-good.git"
  },
  "bugs": {
    "url": "https://github.com/weedz/git-good/issues"
  },
  "homepage": "https://github.com/weedz/git-good#readme"
}
