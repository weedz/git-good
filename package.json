{
  "name": "git-good",
  "version": "0.1.4",
  "description": "Git good",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "build": "node scripts/build.js",
    "build:watch": "mkdir dist; cp -R static/* dist/ && rollup -c -w",
    "electron-rebuild": "electron-builder install-app-deps",
    "lint": "eslint . && tsc --noEmit"
  },
  "keywords": [],
  "author": {
    "name": "Linus BjÃ¶rklund",
    "email": "weedzcokie@gmail.com"
  },
  "files": [
    "dist"
  ],
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/node": "^16.11.41",
    "@types/nodegit": "^0.27.10",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "asar": "^3.1.0",
    "electron": "^19.0.6",
    "electron-builder": "^23.1.0",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "preact": "^10.8.2",
    "rollup": "^2.75.7",
    "rollup-plugin-import-css": "^3.0.3",
    "terser": "^5.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@weedzcokie/store": "^1.0.4",
    "nodegit": "^0.28.0-alpha.18"
  },
  "build": {
    "afterPack": "./scripts/after-pack.js",
    "directories": {
      "output": "out"
    },
    "linux": {
      "category": "Development",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weedz/git-good.git"
  },
  "bugs": {
