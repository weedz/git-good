{
  "name": "git-good",
  "version": "0.1.4",
  "description": "Git good",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron --ozone-platform-hint=wayland .",
    "lint": "eslint . && tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "dev": "NODE_ENV=development npm run build",
    "build": "rm -rf dist;mkdir dist;cp static/* dist/; node scripts/build.mjs",
    "dist": "electron-builder",
    "electron-rebuild": "JOBS=max CXX=\"ccache clang++\" CC=\"ccache clang\" LDFLAGS=-fuse-ld=lld CFLAGS=-flto=thin CXXFLAGS=-flto=thin electron-rebuild"
  },
  "keywords": [],
  "author": {
    "name": "Linus BjÃ¶rklund",
    "email": "weedzcokie@gmail.com"
  },
  "files": [
    "dist"
  ],
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@electron/asar": "^3.2.8",
    "@electron/rebuild": "^3.4.1",
    "@types/node": "^18.19.2",
    "@types/nodegit": "^0.28.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@weedzcokie/store": "^2.0.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "esbuild": "^0.19.8",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "preact": "^10.19.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "nodegit": "github:weedz/nodegit#master"
  },
  "resolutions": {
    "make-fetch-happen": "^13"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  },
  "build": {
    "appId": "git-good",
    "afterPack": "./scripts/after-pack.js",
    "directories": {
      "output": "out"
    },
    "linux": {
      "category": "Development",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weedz/git-good.git"
  },
  "bugs": {
    "url": "https://github.com/weedz/git-good/issues"
  },
  "homepage": "https://github.com/weedz/git-good#readme",
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  }
}
